{
	"arrow function": {
		"prefix": "af",
		"body": [
			"(${2:arguments}) =>  ${0}"
		],
		"description": "Arrow Function inline"
	},
	"arrow function curly braces": {
		"prefix": "afb",
		"body": [
			"(${2:arguments}) => {\n\t${0}\n}"
		],
		"description": "Arrow Function with curly braces"
	},
  "const arrow function assignment": {
    "prefix": "caf",
    "body": "const ${1:name} = (${2:arguments}) => {\n\treturn ${0}\n}"
  }
}
